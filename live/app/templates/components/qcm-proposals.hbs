{{#each labeledCheckboxes as |labeledCheckbox index|}}
  <p class="challenge-proposal">
      <label>
        <input type="checkbox"
          name="{{inc index}}"
          checked={{labeledCheckbox.[1]}} 
          onclick={{action "checkboxClicked"}} />
        {{labeledCheckbox.[0]}}
      </label>
  </p>  
{{/each}}

